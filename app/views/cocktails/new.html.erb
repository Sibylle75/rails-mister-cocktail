<div class="container">
  <div class="row">
    <h1>Enter a cocktail:</h1>
    <%= simple_form_for @cocktail do |f| %>
      <%= f.input :name %>
      <%= f.input :photos, as: :file, input_html: { multiple: true } %>
      <%= f.submit "Enter your cocktail", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
